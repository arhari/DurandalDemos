<section>
	<div class="row-fluid">
		<button type="button" class="btn btn-primary" data-bind="command: refreshCommand">Refresh</button>
	</div>

	<div class="row-fluid">
		<table class="table table-striped table-hover">
			<thead>
				<tr>
					<th>ID</th>
					<th>Name</th>
					<th>Date</th>
					<th>Amount</th>
					<th>Tax</th>
				</tr>
			</thead>
			<tbody data-bind="foreach: model">
				<tr>
					<td data-bind="text: Id"></td>
					<td data-bind="text: Name"></td>
					<td data-bind="text: CalcDate"></td>
					<td data-bind="text: Amount"></td>
					<td data-bind="text: Tax1"></td>
				</tr>
			</tbody>
		</table>
	</div>
</section>
